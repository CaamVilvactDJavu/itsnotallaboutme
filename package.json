{
  "name": "itsnotallaboutme",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "bun run build && concurrently --names \"next, shuttle\" --kill-others \"next dev\" \"cargo shuttle run --working-directory ./backend/ --port 9999\"",
    "build": "next build && cargo build --manifest-path ./backend/Cargo.toml",
    "start": "cargo shuttle project new --working-directory ./backend/",
    "lint": "next lint",
    "shuttle-login": "cargo shuttle login --working-directory ./backend/",
    "deploy": "bun run build && cargo shuttle deploy --working-directory ./backend/ --allow-dirty",
    "stop": "cargo shuttle project rm --working-directory ./backend/"
  },
  "dependencies": {
    "@fortawesome/free-brands-svg-icons": "^6.6.0",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@svgr/webpack": "^8.1.0",
    "autoprefixer": "^10.4.19",
    "next": "14.2.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^9.0.1",
    "remark-gfm": "^4.0.0",
    "sharp": "^0.33.4"
  },
  "devDependencies": {
    "@types/node": "^20.14.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.5",
    "postcss": "^8.4.40",
    "tailwindcss": "^3.4.7",
    "typescript": "^5.5.4"
  }
}
